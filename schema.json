{
  "appdata": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "App Data",
    "description": "Schema for application specific data",
    "type": "object",
    "properties": {
      "app-info": {
        "type": "string",
         "enum": ["red", "green", "blue"],
        "description": "Information about the application."
      },
      "appid": {
        "type": "string",
        "description": "The unique application identifier."
      },
      "dikla": {
        "description": "Any other property."
      }
    },
    "required": [
      "app-info",
      "appid",
      "dikla"
    ]
  },
  "rgula": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Rgula Data",
    "description": "Schema for bank information.",
    "type": "object",
    "properties": {
      "bankname": {
        "type": "string",
        "description": "Name of the bank."
      },
      "bankid": {
        "type": "string",
        "description": "ID of the bank."
      }
    },
    "required": [
      "bankname",
      "bankid"
    ]
  },
  "papa": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Papa Data",
    "type": "object",
    "properties": {
      "gg": {
        "type": "string"
      }
    },
    "required": [
      "gg"
    ]
  }
}
